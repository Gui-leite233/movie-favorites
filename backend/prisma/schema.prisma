generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Favorite {
  id        Int      @id @default(autoincrement())
  title     String
  movieId   Int      @unique
  posterUrl String?
  rating    Float?
  createdAt DateTime @default(now())
}
